#!/bin/bash

alias ga="git add ."
alias gr="git reset"
alias st="git status"
alias gs="git status"
alias gf="git fetch"
alias gp="git pull"
alias pull="git pull"
alias push="git push"
alias branch="git branch"

alias ch-="git checkout -"
alias back="git checkout -"

alias dev="git checkout dev"
alias main="git checkout main"
alias master="git checkout master"

alias uncommit="git reset --soft HEAD^"
alias clearBranches="git branch | grep -vE 'dev|main|master' | xargs git branch -D"
